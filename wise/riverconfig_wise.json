    "{?resource a <https://water.europa.eu/portal_types/News%20Item#NewsItem> } UNION ",
    "{?resource a <https://water.europa.eu/portal_types/EasyForm#EasyForm> } UNION ",
    "{?resource a <https://water.europa.eu/portal_types/Event#Event> } UNION ",
    "{?resource a <https://water.europa.eu/portal_types/Folder#Folder> } "
