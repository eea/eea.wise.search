    "http://purl.org/dc/terms/title": ["title","label"],
    "http://purl.org/dc/terms/abstract": "text",
    "http://purl.org/dc/terms/spatial":["spatial","places"],
    "http://www.w3.org/1999/02/22-rdf-syntax-ns#type": ["type_of_data"],
    "http://purl.org/dc/terms/issued": ["issued", "publication"],
    "http://www.w3.org/1999/02/22-rdf-syntax-ns#about": ["about"],
    "http://purl.org/dc/terms/expires" : ["expires"],
    "http://purl.org/dc/terms/modified" : ["modified"],
    "http://purl.org/dc/terms/description" : ["description"],
    "http://purl.org/dc/terms/format" : ["format"],
    "http://xmlns.com/foaf/0.1/depiction" : ["depiction"],
    "http://www.eea.europa.eu/ontologies.rdf#hasWorkflowState": "hasWorkflowState",
    "http://purl.org/dc/terms/replaces": "replaces",
    "http://purl.org/dc/terms/isReplacedBy": "is_replaced"
  },
  "//": "Normalise the properties to the given value",
  "normalisedProperties": {
  },
  "//": "Do not index these values for a given properties",
  "blackMap": {
    "http://www.w3.org/1999/02/22-rdf-syntax-ns#type": [
      "Webpage",
      "Tracked file",
      "http://www.w3.org/ns/dcat#Dataset",
      "http://www.w3.org/ns/dcat#Distribution",
      "Output from Annual Management Plan"
    ]
  },
  "//": "Index _only_ these values for given properties",
  "whiteMap": {
  },
